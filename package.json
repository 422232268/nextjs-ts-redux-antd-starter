{
  "name": "nextjs-ts-redux-antd-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "coverage": "yarn jest --coverage",
    "commit": "git-cz",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx .",
    "lint:css": "stylelint **/*.{ts,tsx}",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "tsc:client": "tsc --noEmit -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist_storybook",
    "bump-version": "standard-version --skip.commit --skip.tag"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/test-runner": "^0.9.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "antd": "^5.1.2",
    "axios": "^1.2.2",
    "eslint": "^8.31.0",
    "eslint-config-next": "13.1.1",
    "i18next": "^22.4.8",
    "next": "^13.4.3",
    "next-router-provider": "^0.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.2",
    "storybook-addon-next-router": "^4.0.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-jest": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-storyshots": "^6.5.15",
    "@storybook/jest": "0.0.10",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "^29.2.5",
    "@types/react-redux": "^7.1.25",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "add": "^2.0.6",
    "babel-jest": "^29.3.1",
    "babel-plugin-import": "^1.13.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^7.0.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.0",
    "next-redux-wrapper": "^8.1.0",
    "prettier": "^2.8.1",
    "redux-devtools-extension": "^2.13.9",
    "standard-version": "^9.5.0",
    "storybook": "^6.5.15",
    "styled-components": "^5.3.6",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "yarn": "^1.22.19"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 160,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn tsc"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx}": [
      "stylelint",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,scss}": [
      "stylelint",
      "prettier --write"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
